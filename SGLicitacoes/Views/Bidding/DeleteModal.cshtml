@model BiddingInfo
@{
    ViewData["Title"] = "Confirmação apagar Licitação";
}
<div class="text-center">
    <h3>Realmente deseja apagar licitação (@Model.Description)?</h3>
</div>
<a class="btn btn-primary btn-block" role="button" asp-controller="Bidding" asp-action="Index">Cancelar exclusão</a>
<a class="btn btn-danger btn-block" asp-route-id="@Model.Id" role="button" asp-controller="Bidding" asp-action="Delete">Confirmar exclusão</a>