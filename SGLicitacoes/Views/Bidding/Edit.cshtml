@{
    ViewData["Title"] = "Editar Licitações";
}
<div class="text-center">
    <h1 class="display-4">Editar Licitações</h1>
</div>
<form>
    <div class="form-group">
        <label for="description">Descrição</label>
        <input type="text" class="form-control" placeholder="Digite a descrição da sua licitação">
    </div>
    <button type="submit" class="btn btn-primary btn-block">Alterar licitação</button>
    <a class="btn btn-secondary btn-block" asp-asp-controller="Bidding" asp-action="Index" role="button">Voltar</a>
</form>
